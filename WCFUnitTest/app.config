                <binding name="BasicHttpBinding_IArtPieceService" />
                <binding name="BasicHttpBinding_IAuctionService" />
                <binding name="BasicHttpBinding_IBidService" />
                <binding name="BasicHttpBinding_ILotService" />
                <binding name="BasicHttpBinding_IMemberService" />
