<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="BasicHttpBinding_IArtPieceService" />
                <binding name="BasicHttpBinding_IAuctionService" />
                <binding name="BasicHttpBinding_ILotService" />
                <binding name="BasicHttpBinding_IBidService" />
                <binding name="BasicHttpBinding_IMemberService" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8733/Design_Time_Addresses/MainService/ArtPiece"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IArtPieceService"
                contract="MainServiceReference.IArtPieceService" name="BasicHttpBinding_IArtPieceService" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/MainService/Auction"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IAuctionService"
                contract="MainServiceReference.IAuctionService" name="BasicHttpBinding_IAuctionService" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/MainService/Lot"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_ILotService"
                contract="MainServiceReference.ILotService" name="BasicHttpBinding_ILotService" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/MainService/Bid"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IBidService"
                contract="MainServiceReference.IBidService" name="BasicHttpBinding_IBidService" />
            <endpoint address="http://localhost:8733/Design_Time_Addresses/MainService/Member"
                binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IMemberService"
                contract="MainServiceReference.IMemberService" name="BasicHttpBinding_IMemberService" />
        </client>
    </system.serviceModel>
</configuration>